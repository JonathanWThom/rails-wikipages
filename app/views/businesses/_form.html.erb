<%= render('layouts/errors', :object => @business) %>

<%= form_for [@type, @business] do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :address %>
  <%= f.text_field :address %>
  <%= f.label :phone_number %>
  <%= f.text_field :phone_number %>
  <%= f.label :website %>
  <%= f.text_field :website %>
  <%= f.label :hours %>
  <%= f.text_field :hours %>
  <%= f.select :city_id, options_for_select(City.all.collect {|p| [p.name, p.id] }, { prompt: 'Select City'}) %>
  <p><%= f.submit %></p>
<% end %>
